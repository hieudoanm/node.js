{
  "manifest_version": 3,
  "name": "Focus",
  "version": "1.0",
  "description": "Block access to specified websites.",
  "permissions": ["webRequest", "webRequestBlocking", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "index.js"
  },
  "options_page": "options.html"
}
